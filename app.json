{
  "expo": {
    "name": "FriendlyChart",
    "slug": "friendly-chart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "friendchart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "monochromeImage": "./assets/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.friendlychart"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/logo.png"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "dc1a36bc-f1af-4341-8970-e7969fa3913e"
      },
      "EXPO_PUBLIC_API_URL": "https://friend-chat-seven.vercel.app/api",
      "EXPO_PUBLIC_SOCKET_URL": "wss://friendchat-8nfh.onrender.com",
      "EXPO_PUBLIC_API_TIMEOUT": "15000"
    }
  }
}
